.user-info {
	height: 110px;
	border: 1px solid #aaa;
	margin: 5px;
	padding: 4px;
}

.user-info img {
	height: 90px;
}

.user-info .user-left {
	float: left;
	width: 85px;
	padding: 5px;
}

.user-info .user-right {
	float: left;
	width: auto;
	padding: 5px;
}

.user-info .btn-hangout { background: purple; margin-top: 5px; }